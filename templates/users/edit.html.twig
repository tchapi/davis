{% extends 'base.html.twig' %}
{% set menu = 'resources' %}

{% block body %}

{% include '_partials/back_button.html.twig' with { url: path('users'), text: "users.back"|trans } %}

{% if username %}
<h1 class="display-4 fw-lighter mb-5">{{ "users.edit"|trans({'username': username }) }}</h1>
{% else %}
<h1 class="display-4 fw-lighter mb-5">{{ "users.new"|trans }}</h1>
{% endif %}

{{ form(form) }}

{% endblock %}